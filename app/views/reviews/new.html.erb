<h1> New Review </h1> 
<%= form_for(@review) do |f| %>

    <%= f.label :book_title %>
    <%= f.text_field :book_title %>

    <%= f.label :author %>
    <%= f.text_field :author %>

    <%= f.label :review_title %>
    <%= f.text_field :title %>

    <%= f.label :content %>
    <%= f.text_field :content %>

    <%= f.submit %>

    <% end %> 